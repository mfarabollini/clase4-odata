{
  "_version": "1.49.0",
  "sap.app": {
    "id": "exaccs.clase4odata",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.9.7",
      "toolsId": "26602c97-a0c3-4b8d-851e-e123398d6dde"
    },
    "dataSources": {
      "mainService": {
        "uri": "/sap/opu/odata/sap/ZUI5_VUELOS_SRV/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/metadata.xml",
          "odataVersion": "2.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": false,
    "dependencies": {
      "minUI5Version": "1.115.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.f": {},
        "sap.suite.ui.generic.template": {},
        "sap.ui.comp": {},
        "sap.ui.generic.app": {},
        "sap.ui.table": {},
        "sap.ushell": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "exaccs.clase4odata.i18n.i18n"
        }
      },
      "vuelos": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "defaultCountMode": "Inline"
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "exaccs.clase4odata.view",
        "controlAggregation": "pages",
        "controlId": "app",
        "clearControlAggregation": false
      },
      "routes": [
        {
          "name": "RouteMain",
          "pattern": ":?query:",
          "target": [
            "TargetMain"
          ]
        },
        
        {
          "name": "vuelos",
          "pattern": "vuelos/{aerolinea}",
          "target": [
            "VuelosAerolinea"
          ]
        },
        {
          "name": "aerolineas",
          "pattern": "aerolineas",
          "target": [
            "Aerolineas"
          ]
        },
        {
          "name": "itinerarios",
          "pattern": "itinerarios/{cityFrom}/{cityTo}",
          "target": [
            "Itinerarios"
          ]
        }
      ],
      "targets": {
        "TargetMain": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "Main",
          "viewName": "Main"
        },
        "VuelosAerolinea": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "Vuelos",
          "viewName": "Vuelos"
        },
        "Aerolineas": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "Aerolineas",
          "viewName": "Aerolineas"
        },
        "Itinerarios": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "Itinerarios",
          "viewName": "Itinerarios"
        }

      }
    },
    "rootView": {
      "viewName": "exaccs.clase4odata.view.App",
      "type": "XML",
      "async": true,
      "id": "App"
    }
  }
}
